  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "glob": "^7.1.6",
    "js-yaml": "^3.13.1",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "purgecss": "^2.1.2"
  }
}
